<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>

<StackPane xmlns="http://javafx.com/javafx/8.0.171"
           xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="org.example.java_todolist.controller.DashboardController"
           fx:id="rootPane"
           stylesheets="@../Css/Dashboard.css">

    <ScrollPane fx:id="cardScrollPane" fitToWidth="true" style="-fx-background-color: transparent; -fx-border-color: transparent;">
        <content>
            <VBox fx:id="cardContainer" spacing="10">
                <padding>
                    <Insets top="10" right="10" bottom="10" left="10"/>
                </padding>
            </VBox>
        </content>
    </ScrollPane>
</StackPane>

